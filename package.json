{
  "name": "uscc-utils",
  "type": "module",
  "version": "0.7.0",
  "packageManager": "pnpm@10.8.0",
  "description": "Utils about unified social credit code | 统一社会信用代码工具方法",
  "keywords": [
    "parse",
    "unified social credit code",
    "unified social credit identifier",
    "uscc",
    "usci",
    "validate",
    "校验",
    "统一社会信用代码",
    "解析"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/uscc-utils#readme",
  "repository": "ntnyq/uscc-utils",
  "bugs": {
    "url": "https://github.com/ntnyq/uscc-utils/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck build",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.3",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.1"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,yml,yaml,json}": "eslint --fix"
  }
}
