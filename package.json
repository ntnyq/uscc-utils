{
  "name": "uscc-utils",
  "type": "module",
  "version": "0.7.0",
  "packageManager": "pnpm@10.12.4",
  "description": "Utils about unified social credit code | 统一社会信用代码工具方法",
  "keywords": [
    "parse",
    "unified social credit code",
    "unified social credit identifier",
    "uscc",
    "usci",
    "validate",
    "校验",
    "统一社会信用代码",
    "解析"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/uscc-utils#readme",
  "repository": "ntnyq/uscc-utils",
  "bugs": {
    "url": "https://github.com/ntnyq/uscc-utils/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck build",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest --typecheck",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.0.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.0.10",
    "bumpp": "^10.2.0",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,yml,yaml,json}": "eslint --fix"
  }
}
