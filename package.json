{
  "name": "uscc-utils",
  "version": "0.3.1",
  "packageManager": "pnpm@8.6.2",
  "description": "Utils about unified social credit code | 统一社会信用代码工具方法",
  "keywords": [
    "unified social credit code",
    "unified social credit identifier",
    "usci",
    "uscc",
    "parse",
    "validate",
    "统一社会信用代码",
    "解析",
    "校验"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/uscc-utils#readme",
  "bugs": {
    "url": "https://github.com/ntnyq/uscc-utils/issues"
  },
  "repository": "ntnyq/uscc-utils",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck build",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "vitest typecheck"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.9.0",
    "@ntnyq/prettier-config": "^1.9.0",
    "@vitest/coverage-v8": "^0.32.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.6.2",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  }
}
