{
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "yaml",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "codecov",
    "esbuild",
    "lcov",
    "pnpm",
    "uscc",
    "usci",
    "vite",
    "vitest"
  ],
  "cSpell.allowCompoundWords": true
}
